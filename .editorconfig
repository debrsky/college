# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.js]
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = true

[*.html]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 4
